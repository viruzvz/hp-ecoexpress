// Modal
#ciclista.modal.fade(tabindex='-1', role='dialog', aria-labelledby='ciclistaLabel')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') ×
        h4#ciclistaLabel.modal-title Ciclista Integral
      .modal-body
        table.table.table-hover
          caption: strong Modelos de pagamento
          thead
            tr
              th #
              th Ciclista Basic (40km/dia)
              th Ciclista Intermediate (60km/dia)
              th Ciclista Expert (80km/dia)
          tbody
            tr
              th(scope='row') 10 dias
              td R$100,00
              td R$200,00
              td R$300,00
            tr
              th(scope='row') 20 dias
              td R$300,00
              td R$400,00
              td R$500,00
            tr
              th(scope='row') 30 dias
              td R$600,00
              td R$700,00
              td R$800,00
      .modal-footer
        button.btn.btn-default(type='button', data-dismiss='modal') Fechar
